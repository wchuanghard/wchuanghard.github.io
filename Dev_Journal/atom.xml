---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>William Chuang's Dev Journal</title>
	<link href="http://wchuanghard.github.io/Dev_Journal/atom.xml" rel="self"/>
	<link href="http://wchuanghard.github.io/Dev_Journal"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://wchuanghard.github.io/Dev_Journal</id>
	<author>
		<name>William Chuang</name>
		<email>wchuang@g.harvard.edu</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ Dev_Journal.title }}</title>
			<link href="http://wchuanghard.github.io{{ Dev_Journal.url }}"/>
			<updated>{{ Dev_Journal.date | date_to_xmlschema }}</updated>
			<id>http://wchuanghard.github.io/{{ Dev_Journal.id }}</id>
			<content type="html">{{ Dev_Journal.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
